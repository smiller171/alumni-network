---
extends:
  - react-app
plugins:
  - jsx-a11y
  - react
rules:
  sort-imports:
  - error
  - ignoreCase: true
    ignoreMemberSort: false
    memberSyntaxSortOrder:
    - none
    - all
    - single
    - multiple
  sort-vars:
  - error
  - ignoreCase: true
  sort-keys:
  - warn
  - asc
  - caseSensitive: false
    natural: true
  jsx-a11y/href-no-hash: 0
